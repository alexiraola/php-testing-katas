<?xml version="1.0"?>
<ruleset name="Custom Ruleset">
    <description>Custom ruleset to suppress doc comment warnings.</description>

    <!-- Include your standard (e.g., PSR12) -->
    <rule ref="PSR12"/>

    <!-- Exclude doc comment warnings -->
    <rule ref="Generic.Commenting.DocComment">
        <exclude name="Generic.Commenting.DocComment.Missing"/>
    </rule>
</ruleset>

